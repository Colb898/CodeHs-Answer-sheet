#moves the dog up the tower
def make_tower():
    move()
    put_ball()
    turn_left()
    move()
    put_ball()
    move()
    put_ball()
    turn_left()
    turn_left()
    turn_left()
    #move the dog down and up the tower
def down_tower():
    move()
    turn_left()
    turn_left()
    turn_left()
    move()
    move()
    turn_left()
    move()
    put_ball()
    turn_left()
    move()
    put_ball()
    move()
    put_ball()
    move()
    turn_left()
    turn_left()
    turn_left()
make_tower()
down_tower()
